    "# Hw 2 pyramid\n",
    "\n",
    "def pyramid(n):\n",
    "    for i,v in enumerate(range(n)):\n",
    "        print(f'{\" \"* (n - i)}{\"*\"*(i*2+1)}{\" \"* (n - i)}')\n",
    "    \n",
    "\n",
    "n = 5\n",
    "pyramid(5)\n"
   ]
